<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.twitstreet.db.data">
	<class name="UserStockDO" table="userstock" lazy="false">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<many-to-one name="user" column="user" not-null="true" unique-key="UK_PORTFOLIO"></many-to-one>
		<many-to-one name="stock" column="stock" not-null="true" unique-key="UK_PORTFOLIO"></many-to-one>
		<property name="percent" column="percent" type="double" not-null="true"></property>
	</class>
</hibernate-mapping>