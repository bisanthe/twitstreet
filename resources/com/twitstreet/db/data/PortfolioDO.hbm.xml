<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.twitstreet.db.data">
	<class name="PortfolioDO" table="portfolio" lazy="false">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="stock" column="stock" type="integer" not-null="true"></property>
		<property name="user" column="user" type="integer" not-null="true"></property>
		<property name="amount" column="amount" type="integer" not-null="true"></property>
	</class>
</hibernate-mapping>