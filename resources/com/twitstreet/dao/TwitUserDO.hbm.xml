<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.twitstreet.dao">
	<class name="TwitUSerDO" table="twituser" lazy="false">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="follower" column="follower" type="integer" not-null="true"></property>	
		<property name="sold" column="sold" type="integer" not-null="true"></property>	
	</class>
</hibernate-mapping>