<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.twitstreet.dao">
	<class name="PortfolioDO" table="portfolio" lazy="false">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="twituser" column="twituser_id" type="integer" not-null="true"></property>
		<property name="user" column="user_id" type="integer" not-null="true"></property>
		<property name="percent" column="percent" type="double" not-null="true"></property>
	</class>
</hibernate-mapping>