<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.twitstreet.db.data">
	<class name="StockDO" table="stock" lazy="false">
		<id name="id" column="id" />
		<property name="name" column="name" type="string" not-null="true"></property>
		<property name="total" column="total" type="integer" not-null="true"></property>	
		<property name="percentSold" column="percentSol" type="double" not-null="true"></property>	
	</class>
</hibernate-mapping>